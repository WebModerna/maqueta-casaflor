#fancybox-thumbs
	left 0
	overflow hidden
	position fixed
	width 100%
	z-index 8050

#fancybox-thumbs.bottom 
	bottom 2px

#fancybox-thumbs.top 
	top 2px

#fancybox-thumbs ul 
	list-style none
	margin 0
	padding 0
	position relative

#fancybox-thumbs ul li 
	float left
	padding 1px
	opacity 0.5

#fancybox-thumbs ul li.active 
	border 1px solid #fff
	opacity 0.75
	padding 0

#fancybox-thumbs ul lihover 
	opacity 1

#fancybox-thumbs ul li a 
	background #111
	border 1px solid #222
	display block
	outline none
	overflow hidden
	position relative

#fancybox-thumbs ul li img 
	border 0
	display block
	padding 0
	position relative